  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --config webpack/dev.config.js --mode development --color",
    "build:client:qa": "rimraf dist/homepage && NODE_ENV=production webpack  --config webpack/qa.client.config.js --progress --color",
    "build:client:prod": "rimraf dist/homepage && NODE_ENV=production webpack  --config webpack/prod.client.config.js --progress --color",
    "deploy:client:qa": "npm run build:client:qa && node scripts/qa.s3.js",
    "deploy:client:prod": "npm run build:client:prod && node scripts/prod.s3.js",
    "build:ssr:client:qa": "rimraf server-static/$APP && NODE_ENV=production webpack  --config webpack/qa.ssr.client.config.js --progress --color",
    "build:ssr:server:qa": "rimraf lambda-server/$APP && NODE_ENV=production webpack  --config webpack/qa.server.config.js --progress --color",
    "build:ssr:qa": "npm run build:ssr:client:qa && npm run build:ssr:server:qa",
    "build:ssr:client:prod": "rimraf server-static/$APP && NODE_ENV=production webpack  --config webpack/prod.ssr.client.config.js --progress --color",
    "build:ssr:server:prod": "rimraf lambda-server/$APP && NODE_ENV=production webpack  --config webpack/prod.server.config.js --progress --color",
    "build:ssr:prod": "npm run build:ssr:client:prod && npm run build:ssr:server:prod",
    "build:client:dev": "webpack --config webpack/dev.client.config.js --mode development --color",
    "build:server:dev": "webpack --config webpack/dev.server.config.js --mode development --color",
    "build:ssr:dev": "npm run build:client:dev && npm run build:server:dev",
    "run:ssr:dev": "npm run build:ssr:dev && node server/ssr/api/server.js"
  },
